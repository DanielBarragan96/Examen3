module ShiftLeft2 
(   
	input [25:0]  DataInput,
   output reg [31:0] DataOutput

);
   always @ (DataInput)
     DataOutput = {1'b0, 1'b0, 1'b0, 1'b0, 1'b0, 1'b0, DataInput[25:0], 1'b0, 1'b0};

endmodule // leftShift2